#include <stdio.h>

int main() {
    int n, i;
    float rain[50], total = 0, average;

    printf("Enter number of days: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Enter rainfall for day %d: ", i + 1);
        scanf("%f", &rain[i]);
        total = total + rain[i];
    }

    average = total / n;

    printf("Average Rainfall = %.2f\n", average);

    return 0;
}