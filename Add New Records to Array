#include <stdio.h>

int main() {
    int a[10], n, i, newVal;

    printf("Enter size: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);

    printf("Enter new value: ");
    scanf("%d", &newVal);

    a[n] = newVal;
    n++;

    printf("Updated Array:\n");
    for(i = 0; i < n; i++)
        printf("%d ", a[i]);

    return 0;
}