#include <stdio.h>

float area(float l, float b) {
    return l * b;
}

int main() {
    float length, breadth, result;

    printf("Enter length and breadth: ");
    scanf("%f %f", &length, &breadth);

    result = area(length, breadth);

    printf("Area of Rectangle = %.2f\n", result);

    return 0;
}