#include <stdio.h>

int main() {
    int n, i, present = 0;
    int status[50];  // 1 = present, 0 = absent

    printf("Enter number of students: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Enter 1 if student %d is present, 0 if absent: ", i + 1);
        scanf("%d", &status[i]);
        if (status[i] == 1)
            present++;
    }

    printf("Number of present students = %d\n", present);

    return 0;
}