#include <stdio.h>

int main() {
    char str[100], newstr[100];
    int i = 0, j = 0;

    printf("Enter a string: ");
    fgets(str, 100, stdin);

    while (str[i] != '\0') {
        if (str[i] != ' ') {
            newstr[j] = str[i];
            j++;
        }
        i++;
    }
    newstr[j] = '\0';

    printf("String without spaces: %s\n", newstr);

    return 0;
}