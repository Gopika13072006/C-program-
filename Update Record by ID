#include <stdio.h>

struct Item {
    int id;
    char name[50];
};

int main() {
    struct Item it[10];
    int n, i, uid;

    printf("Enter number of items: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++)
        scanf("%d %s", &it[i].id, it[i].name);

    printf("Enter ID to update: ");
    scanf("%d", &uid);

    for(i = 0; i < n; i++) {
        if(it[i].id == uid) {
            printf("Enter new name: ");
            scanf("%s", it[i].name);
            printf("Updated: %s\n", it[i].name);
            return 0;
        }
    }

    printf("Record not found\n");
    return 0;
}