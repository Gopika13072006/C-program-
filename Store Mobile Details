#include <stdio.h>

struct Mobile {
    char brand[50];
    char model[50];
    float price;
};

int main() {
    struct Mobile m;

    printf("Enter brand: ");
    scanf("%s", m.brand);

    printf("Enter model: ");
    scanf("%s", m.model);

    printf("Enter price: ");
    scanf("%f", &m.price);

    printf("\nMobile Details:\n");
    printf("Brand: %s\nModel: %s\nPrice: %.2f\n", m.brand, m.model, m.price);

    return 0;
}